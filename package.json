{
  "name": "reactboilerplate",
  "version": "1.0.0",
  "authors": ["Adam Cunard","Jeremy Cox"],
  "description": "Boilerplate React APP",
  "main": "public/main.js",
  "homepage": "./",
  "dependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron-context-menu": "^3.6.1",
    "electron-is-dev": "^2.0.0",
    "electron-router-dom": "^1.0.5",
    "electron-store": "^8.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "style-loader": "^3.3.3",
    "styled-components": "^6.1.8"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack .",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm run electron:start\"",
    "electron:start": "wait-on http://127.0.0.1:3001 && electron .",
    "electron:solo": "electron .",
    "electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/main.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "electron": "^26.2.1",
    "electron-builder": "^24.6.4",
    "babel-plugin-styled-components": "^2.1.4",
    "wait-on": "^7.0.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "build": {
    "appId": "com.react.boilerplate",
    "productName": "Boilerplate App",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "target": "nsis"
    }
  },
  "electronWebPack": {
    "whiteListedModules": [
      "react-router-dom"
    ]
  }

}
